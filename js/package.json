{
  "name": "@aditya-wiguna/flarum-jwt-sso",
  "version": "1.0.0",
  "description": "JWT-based SSO extension for Flarum",
  "main": "js/dist/forum.js",
  "scripts": {
    "dev": "webpack --mode development --watch",
    "build": "webpack --mode production",
    "analyze": "cross-env ANALYZER=true npm run build",
    "format": "prettier --write js/src/**/*.js",
    "format:check": "prettier --check js/src/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aditya-wiguna/flarum-jwt-sso.git"
  },
  "keywords": [
    "flarum",
    "extension",
    "sso",
    "jwt",
    "authentication"
  ],
  "author": "I Wayan Aditya Wiguna",
  "license": "MIT",
  "devDependencies": {
    "flarum-webpack-config": "^2.0.0",
    "webpack": "^5.0.0",
    "webpack-cli": "^4.0.0",
    "cross-env": "^7.0.3",
    "prettier": "^2.8.0"
  }
}